{
    "name": "sfachinger/test-bundle",
    "description": "This is only a test",
    "keywords": [
        "contao",
        "bundle",
        "tag 3",
        "tag 4"
    ],
    "type": "contao-bundle",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Steffen Fachinger",
            "email": "steffen.fachinger@online.de",
            "homepage": "www.kreuzbund-dv-limburg.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/sfachinger/test-bundle/issues",
        "source": "https://github.com/sfachinger/test-bundle"
    },
    "require": {
        "contao/core-bundle": "^4.9"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.3",
        "contao/test-case": "^4.0",
        "phpunit/phpunit": "^8.4",
        "symfony/http-client": "4.4.*",
        "symfony/phpunit-bridge": "4.4.*",
        "contao/easy-coding-standard": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Sfachinger\\TestBundle\\": "src/"
        },
        "classmap": [
            "src/Resources/contao"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config",
            "src/Resources/contao/dca",
            "src/Resources/contao/languages",
            "src/Resources/contao/templates"
        ]
    },
    "extra": {
        "contao-manager-plugin": "Sfachinger\\TestBundle\\ContaoManager\\Plugin"
    }
}
